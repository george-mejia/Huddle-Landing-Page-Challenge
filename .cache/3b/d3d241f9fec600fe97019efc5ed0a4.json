ont-family: 'Poppins', sans-serif;\n  background-image: url(./images/bg-mobile.svg);\n  background-repeat: no-repeat;\n  background-size: cover;\n  width: 100%;\n  min-height: 100vh;\n  background-color: var(--violet);\n}\n\n.container {\n  width: 80%;\n  margin: auto;\n}\n\n.nav {\n  width: 100%;\n  padding: 40px 0 50px;\n}\n\n.nav-logo {\n  width: 100%;\n  max-width: 9rem;\n}\n\n.grid {\n  display: grid;\n  row-gap: 80px;\n  column-gap: 50px;\n  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));\n}\n\n.hero-content {\n  width: 100%;\n  height: 100%;\n}\n\n.hero-image {\n  width: 100%;\n  height: auto;\n}\n\n.hero-title {\n  color: var(--white);\n  margin-bottom: 20px;\n  text-transform: capitalize;\n  font-size: 1.5rem;\n  font-weight: 700;\n  text-align: center;\n}\n\n.hero-subtitle {\n  color: var(--white);\n  margin-bottom: 20px;\n  line-height: 1.7;\n  font-size: 1rem;\n  font-weight: 400;\n  text-align: center;\n}\n\n.hero-button {\n  background-color: var(--white);\n  color: var(--violet);\n  text-transform: capitalize;\n  display: inline-block;\n  padding: 1rem 2rem;\n  border-radius: 30px;\n  width: 100%;\n  max-width: 230px;\n  font-size: 1rem;\n  margin-bottom: 50px;\n}\n\n.hero-links {\n  display: flex;\n  width: 100%;\n  justify-content: center;\n  margin-bottom: 50px;\n}\n\n.hero-social-link {\n  text-decoration: none;\n  color: var(--white);\n  border: 1px solid var(--white);\n  width: 50px;\n  height: 50px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 50px;\n  margin: 0 8px;\n}\n\n.hero-content--center {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\nfooter {\n  background-color: inherit;\n  color: var(--white);\n  text-align: center;\n  padding: 10px 0 30px;\n}\n\n.footer-link {\n  color: var(--blue);\n}\n\n@media screen and (min-width: 48em) {\n  .hero-links {\n    height: 400px;\n    max-height: 200px;\n    justify-content: flex-end;\n    align-items: flex-end;\n  }\n}\n\n@media screen and (min-width: 60em) {\n  .container {\n    width: 75%;\n  }\n\n  header {\n    background-image: url(./images/bg-desktop.svg);\n    background-repeat: no-repeat;\n    background-size: cover;\n    width: 100%;\n    min-height: 100vh;\n    background-color: var(--violet);\n  }\n\n  .hero-content--center {\n    justify-content: center;\n  }\n\n  .hero-content--left {\n    align-items: flex-start;\n  }\n\n  .hero-title, .hero-subtitle  {\n    text-align: start;\n  }\n\n  .hero-links--right {\n    justify-content: flex-end;\n  }\n}\n\n\n\n"},"lineCount":null}},"error":null,"hash":"b4ddc15e466085cd3e2f8ff60700542f","cacheData":{"env":{}}}